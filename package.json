{
  "name": "create-make",
  "version": "0.1.2-test",
  "description": "An advance CLI tools for creating new project from GitHub repository.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/z-npm/vite-plugin-swc.git"
  },
  "author": {
    "name": "Zero <github.com/z-nerd>"
  },
  "license": "MIT",
  "keywords": [
    "starter maker",
    "project maker"
  ],
  "files": [
    "bin.js"
  ],
  "main": "bin.js",
  "bin": {
    "create-make": "./bin.js"
  },
  "scripts": {
    "build": "yarn swc src -o bin.js && chmod +x bin.js",
